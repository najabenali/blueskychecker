{
  "version": 2,
  "builds": [
    {
      "src": "src/index.jsx",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.jsx"
    }
  ]
}
